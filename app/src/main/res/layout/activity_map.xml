<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.poornima.aarohan.aarohan2017.MapActivity" >
    <fragment
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment" />
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabpce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/fabpiet"
        android:layout_alignStart="@+id/fabpiet"
        app:backgroundTint="#fff"
        android:src="@drawable/ic_map_fab_clg_24dp"/>
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabpiet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/fabpgi"
        android:layout_alignStart="@+id/fabpgi"
        android:layout_marginTop="15dp"
        app:backgroundTint="#fff"
        android:src="@drawable/ic_map_fab_events_24dp"/>
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabpgi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="80dp"
        app:backgroundTint="#fff    "
        android:src="@drawable/ic_map_fab_food_24dp" />
</RelativeLayout>